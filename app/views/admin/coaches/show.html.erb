<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @coach.name %>
</p>

<p>
  <strong>Title:</strong>
  <%= @coach.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @coach.description %>
</p>

<p>
  <strong>Images:</strong>
  <ul>
  	<% @coach.images.each do |image| %>
  		<li><%= image_tag(image.url) %></li>
  	<% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_admin_coach_path(@coach) %> |
