<% content_for :head_top do %>
    <title>Thank You</title>
    <meta name="description" content="Thank you page after submitting a form">
<% end %>

<header style="background-image: url('https://cftaskforce.files.wordpress.com/2016/01/barbells.jpg');">
    <div class="intro-content">
        <div class="brand-name">You Rock!</div>
        <hr class="colored">
        <div class="brand-name-intro">Form Successfully Submitted<div>        
    </div>
</header>

<section>
    <div class="container-fluid">
        <div class="row text-center">
            <div class="col-lg-12">
                <h2>See What We're Up To</h2>
                <hr class="colored">
            </div>
        </div>
        <div class="row content-row text-center">
        	<div class="col-lg-8 col-lg-offset-2">
                <%= link_to("/blog", target: "_blank") do %>
                    <div class="col-md-4 admin-nav-block">
                        <h1 style="font-size:35px">Blog</h1>
                        <%= fa_icon "rss", class: "fa-4x" %>
                    </div>
                <% end %>
                <%= link_to("https://www.facebook.com/cigarcitycrossfit/", target: "_blank") do %>
                    <div class="col-md-4 admin-nav-block">
                        <h1 style="font-size:35px">Facebook</h1>
                        <%= fa_icon "facebook", class: "fa-4x" %>
                    </div>
                <% end %>
                <%= link_to("https://www.instagram.com/cigarcitycrossfit/", target: "_blank") do %>
                    <div class="col-md-4 admin-nav-block">
                        <h1 style="font-size:35px">Instagram</h1>
                        <%= fa_icon "instagram", class: "fa-4x" %>
                    </div>
                <% end %>
        	</div>
        </div>
    </div>
</section>
