<%= render "shared/navbar" %>
<body id="page-top" class="<%= controller_name %> <%= action_name %>">
  <%= yield %>
  <script id="dsq-count-scr" src="//cftaskforce.disqus.com/count.js" async></script>
</body>
<%= render "shared/footer" %>
</html>
